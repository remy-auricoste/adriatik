{
  "libs": [
    "react",
    "react-dom",
    "rauricoste-arrays",
    "rauricoste-random",
    "rauricoste-commandify",
    "rauricoste-logger",
    "rauricoste-request",
    "rauricoste-store-sync"
  ],
  "dist": "dist",
  "modules": {
    "main": {
      "src": "src",
      "browserify": {
        "entry": "main.js",
        "output": "bundle.js"
      },
      "sass": {
        "entry": "style/main.scss",
        "output": "bundle-sass.css"
      },
      "assets": ["txt", "jpg", "ttf", "png", "html"],
      "watch": true
    }
  }
}
